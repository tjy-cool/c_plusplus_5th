// 将设置放入此文件中以覆盖默认值和用户设置。
{
    "clang.executable":  "D:/Program Files/LLVM/bin/clang.exe",
    "clang.completion.enable": true,
    "clang.cxxflags": [
        "-std=c++11", "-I/path/to/my/include/directory"
    ],
    "files.associations": {
        "iostream": "cpp"
    },

    // "astyle.astylerc": null,
    "astyle.astylerc": "${workspaceRoot}/.vscode/asytlerc}",
    "astyle.cmd_options": [
        "--indent=tab", "--break-blocks"
    ]

}